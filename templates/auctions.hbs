<div class="row">
  <div class="col-xs-6">
    <h1>Auctions</h1>
  </div>

  <div class="col-xs-6">
    <div class="pull-right">
      {{#link-to 'new-auction' classNames="btn btn-info"}}Add new auction{{/link-to}}
    </div>
  </div>
</div>

<ul>
  {{#each auction in model}}
    <li>{{auction.name}} {{auction.date}} {{auction.location}}</li>
  {{/each}}
</ul>
