<h2>Add a new Lot</h2>
<form>
  <div class="form-group">
    <label for="year">Year</label>
    {{input value=year id="year" class="form-control"}}
  </div>
  <div class="form-group">
    <label for="make">Make</label>
    {{input value=make id="make" class="form-control"}}
  </div>
  <div class="form-group">
    <label for="modelName">Model</label>
    {{input value=modelName id="modelName" class="form-control"}}
  </div>
  <div class="form-group">
    <label for="estimatedValueLow">Estimated Value Low</label>
    {{input value=estimatedValueLow id="estimatedValueLow" class="form-control"}}
    <label for="estimatedValueHigh">Estimated Value High</label>
    {{input value=estimatedValueHigh id="estimatedValueHigh" class="form-control"}}
  </div>
  <div class="form-group">
    <label for="photoUrl">Photo URL</label>
    {{input value=photoUrl id="photoUrl" class="form-control"}}
  </div>
  <button class="btn btn-primary" {{action 'save'}}>Add Lot</button>
</form>
